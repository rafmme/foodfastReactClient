{
    "name": "foodfastreactclient",
    "version": "1.0.0",
    "description": "this is an implement of the foodfast client [front-end] with the ReactJS library",
    "main": "index.js",
    "scripts": {
        "start:dev": "webpack-dev-server --config webpack.dev.js --open --hot",
        "build": "webpack --config webpack.prod.js",
        "prestart": "npm run build",
        "start": "nodemon --exec babel-node server/app.js",
        "test": "jest --u",
        "test:watch": "jest --watch",
        "test:coverage": "jest --u --coverage --colors && cat ./coverage/lcov.info | coveralls",
        "prettier": "prettier --write src/**/*.js",
        "lint": "eslint --debug src/",
        "lint-write": "eslint --debug src/ --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": ""
        }
    },
    "lint-staged": {
        "*.(js|jsx)": [
            "npm run prettier",
            "npm run lint-write",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rafmme/foodfastReactClient.git"
    },
    "keywords": [
        "react",
        "redux",
        "react-router"
    ],
    "author": "Timileyin Farayola",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/rafmme/foodfastReactClient/issues"
    },
    "homepage": "https://github.com/rafmme/foodfastReactClient#readme",
    "dependencies": {
        "@babel/node": "^7.2.2",
        "@babel/runtime": "^7.2.0",
        "axios": "^0.18.0",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "google-map-react": "^1.1.2",
        "history": "^4.7.2",
        "jwt-decode": "^2.2.0",
        "keymirror": "^0.1.1",
        "prop-types": "^15.6.2",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-redux": "^6.0.0",
        "react-router-dom": "^4.3.1",
        "react-toastify": "^4.5.2",
        "react-toastr": "^3.0.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "yup": "^0.26.6"
    },
    "devDependencies": {
        "@babel/core": "^7.2.0",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "acorn": "^6.0.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^1.0.0",
        "coveralls": "^3.0.2",
        "css-loader": "^2.0.0",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "eslint": "^5.9.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-jest-enzyme": "^7.0.1",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jest": "^22.1.2",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "favicons-webpack-plugin": "0.0.9",
        "file-loader": "^2.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.2.0",
        "jest": "^23.6.0",
        "lint-staged": "^8.1.0",
        "nodemon": "^1.18.9",
        "prettier": "^1.15.3",
        "pretty-quick": "^1.8.0",
        "react-test-renderer": "^16.6.3",
        "redux-logger": "^3.0.6",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.1.0",
        "webpack": "^4.27.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "webpack-merge": "^4.1.5"
    },
    "engines": {
        "npm": "6.4.1",
        "node": "10.14.1"
    }
}
